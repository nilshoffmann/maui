                <attr intvalue="0" name="position"/>
            </file>
            <file name="de-mdcberlin-test-SomeAction-separatorAfter.instance">
                <!--de.mdcberlin.test.SomeAction-->
                <attr name="instanceCreate" newvalue="javax.swing.JSeparator"/>
                <attr intvalue="50" name="position"/>
            </file>
            <file name="de-mdcberlin-test-SomeAction-separatorBefore.instance">
                <!--de.mdcberlin.test.SomeAction-->
                <attr name="instanceCreate" newvalue="javax.swing.JSeparator"/>
